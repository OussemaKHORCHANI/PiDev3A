{{ form_start(form,{'attr': {'novalidate': 'novalidate'}}) }}
<table ALIGN="Center" >
    <tr><td>{{ form_row(form.nom ,{'label':'Nom'}) }}</td></tr>
    <tr><td>{{ form_row(form.prenom ,{'label':'Prenom'}) }}</td></tr>
    <tr><td>{{ form_row(form.address ,{'label':'Adresse'}) }}</td></tr>
    <tr><td>{{ form_row(form.numtelc ,{'label':'Numero de Telephone'}) }}</td></tr>
    <tr><td>{{ form_row(form.email ,{'label':'Email'}) }}</td></tr>
    <tr><td>{{ form_row(form.mdp ,{'label':'Mot de Passe'}) }}</td></tr>
    <tr><td >{{ form_row(form.confirm_mdp ,{'label':'Repetez Votre Mot de Pass'}) }}</td></tr>
