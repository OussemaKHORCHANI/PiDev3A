<form method="post" action="{{ path('terrain_delete', {'idterrain': terrain.idterrain}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer ce terrain?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ terrain.idterrain) }}">
    <button class="btn btn btn-danger">Supprimer</button>
</form>
