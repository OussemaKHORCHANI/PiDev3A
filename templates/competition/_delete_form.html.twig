<form method="post" action="{{ path('competition_delete', {'idCompetition': competition.idCompetition}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ competition.idCompetition) }}">
    <button class="btn btn-danger">Delete</button>
</form>
